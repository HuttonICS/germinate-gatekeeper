<ui:UiBinder xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:ui='urn:ui:com.google.gwt.uibinder'>
	<g:FlowPanel>
		<b:Modal closable="false" dataBackdrop="STATIC" fade="true" removeOnHide="true" ui:field="modal">
			<b:ModalBody ui:field="body">
				<b:Progress active="true" type="STRIPED">
					<b:ProgressBar percent="100" type="SUCCESS" ui:field="progress"/>
				</b:Progress>
			</b:ModalBody>
		</b:Modal>
	</g:FlowPanel>
</ui:UiBinder>